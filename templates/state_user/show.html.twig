{% extends 'base.html.twig' %}

{% block title %}StateUser{% endblock %}

{% block body %}
    <div class="d-flex flex-sm-row flex-column">
        <div class="mr-auto p-2">
            <a class="btn-action btn-back" href="{{ path('state_user_index') }}"><span class="icon-Back"></span></a>
            <h1>State User</h1>
        </div>
        <div class="p-2"><a class="btn-action" href="{{ path('state_user_edit', {'id': state_user.id}) }}"><span class="icon icon-Pen"></span> Edit</a></div>
        <div class="p-2">{{ include('state_user/_delete_form.html.twig') }}</div>
    </div>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ state_user.id }}</td>
            </tr>
            <tr>
                <th>DateCreate</th>
                <td>{{ state_user.dateCreate ? state_user.dateCreate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>


{% endblock %}
